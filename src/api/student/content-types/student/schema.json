{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "student",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "studentName": {
      "type": "string",
      "required": true
    },
    "roomNumber": {
      "type": "integer",
      "default": 0
    },
    "mobileNumber": {
      "type": "string",
      "regex": "^[6-9]\\d{9}$",
      "required": true
    },
    "guardianName": {
      "type": "string",
      "required": true
    },
    "guardianPhoneNumber": {
      "type": "string",
      "regex": "^[6-9]\\d{9}$",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "govId": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "govIdImage": {
      "type": "text",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "student"
    },
    "Gym": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::gym.gym",
      "mappedBy": "student"
    },
    "complaints": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::complaint.complaint",
      "mappedBy": "student"
    }
  }
}
